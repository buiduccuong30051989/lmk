{
  "name": "react-vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --apply src/",
    "format": "biome format --write src/",
    "preview": "vite preview",
    "check": "biome check src/",
    "type-check": "tsc --noEmit",
    "validate-data": "node -e \"import('./src/data/dataValidator.js').then(m => console.log(m.getComprehensiveStats()))\"",
    "build:analyze": "vite build --mode analyze",
    "clean": "rm -rf dist node_modules/.vite",
    "deploy:vercel": "pnpm build && vercel --prod",
    "deploy:netlify": "pnpm build && netlify deploy --prod --dir=dist",
    "deploy:gh": "pnpm build && gh-pages -d dist",
    "predeploy": "pnpm build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-youtube": "^10.1.0",
    "gsap": "^3.12.2",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0",
    "class-variance-authority": "^0.7.0",
    "lucide-react": "^0.294.0",
    "@radix-ui/react-slot": "^1.0.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.1.1",
    "vite": "^5.0.0",
    "typescript": "^5.2.2",
    "@biomejs/biome": "^1.4.1",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss-animate": "^1.0.7",
    "gh-pages": "^6.1.0"
  }
}